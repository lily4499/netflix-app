{
  "pipeline": {
    "name": "NetflixAppPipeline",
    "roleArn": "arn:aws:iam::637423529262:role/CodeBuildKubectlRole",
    "artifactStore": {
      "type": "S3",
      "location": "lili-codepipeline-artifact-bucket"
    },
    "stages": [
      {
        "name": "Source",
        "actions": [
          {
            "name": "SourceAction",
            "actionTypeId": {
              "category": "Source",
              "owner": "ThirdParty",
              "provider": "GitHub",
              "version": "1"
            },
            "runOrder": 1,
            "configuration": {
                "Owner": "lily4499",
                "Repo": "netflix-app",
                "Branch": "main",
                "OAuthToken": "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",
                "PollForSourceChanges": "false"  
            },
            "outputArtifacts": [
                {
                    "name": "SourceArtifact"
                }
            ]
          }
        ]
      },
      {
        "name": "Build",
        "actions": [
          {
            "name": "Build",
            "actionTypeId": {
              "category": "Build",
              "owner": "AWS",
              "provider": "CodeBuild",
              "version": "1"
            },
            "inputArtifacts": [{ "name": "SourceArtifact" }],
            "outputArtifacts": [{ "name": "BuildArtifact" }],
            "configuration": {
              "ProjectName": "NetflixAppBuild"
            }
          }
        ]
      }
    ]
  }
}

